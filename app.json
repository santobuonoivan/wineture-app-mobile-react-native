{
  "expo": {
    "name": "wineture",
    "slug": "wineture",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/wineture_icon_ios.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "wineture",
    "splash": {
      "image": "./assets/wineture_icon_ios.png",
      "resizeMode": "contain",
      "backgroundColor": "#6B0F1A"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/wineture_icon_ios.png",
        "backgroundColor": "#6B0F1A"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.anonymous.wineture"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}